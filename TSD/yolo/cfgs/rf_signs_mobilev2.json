{
    "model" : {
        "main_name":            "TSD_YOLO_MN2",
        "min_input_size":       [160, 192],
        "max_input_size":       [256, 288],
        "base":                 "MobileNetv2_35",
        "anchors":              [29,29, 40,44, 61,64],
        "labels":               ["brick", "forward", "forward_left", "forward_right", "left", "right", "traffic_light"],
        "infer_shape":          [224, 256]
    },

    "train": {
        "train_image_folder":   ["../rf_tsd_voc/RF19/Images/", "../rf_tsd_voc/RF17/Images/", "../rf_tsd_voc/Uni/Images/"],
        "train_annot_folder":   ["../rf_tsd_voc/RF19/Annotations_train/", "../rf_tsd_voc/RF17/Annotations_train/", "../rf_tsd_voc/Uni/Annotations_train"],
        "cache_name":           "",

        "train_times":          5,
        "batch_size":           4,
        "learning_rate":        1e-4,
        "nb_epochs":            1000,
        "warmup_epochs":        0,
        "ignore_thresh":        0.5,
        "gpus":                 "0",

        "grid_scales":          [1,1,1],
        "obj_scale":            5,
        "noobj_scale":          1,
        "xywh_scale":           1,
        "class_scale":          1,

        "debug":                false
    },

    "valid": {
        "valid_image_folder":   "../rf_tsd_voc/Images/",
        "valid_annot_folder":   "../rf_tsd_voc/Annotations_test",
        "cache_name":           "pkl/sign_test.pkl",

        "valid_times":          1
    },

    "eval": {
        "valid_image_folder":   "../rf_tsd_voc/Images/",
        "valid_annot_folder":   "../rf_tsd_voc/Annotations_test",
        "cache_name":           "pkl/sign_eval.pkl"
    }
}
