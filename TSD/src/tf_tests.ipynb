    "import tensorflow as tf\n",
    "\n",
    "w=10\n",
    "h=5\n",
    "\n",
    "sess = tf.Session()\n",
    "\n",
    "t_width = tf.range(w)\n",
    "t_mtrx_x = tf.reshape(tf.tile(t_width, [h]), [1, h, w, 1, 1])\n",
    "print(sess.run(t_mtrx_x))\n",
    "\n",
    "t_height = tf.range(h)\n",
    "t_mtrx_y = tf.reshape(tf.tile(t_height, [w]), [1, w, h, 1, 1])\n",
    "print(sess.run(t_mtrx_y))\n",
    "\n",
    "\n"
   ]
