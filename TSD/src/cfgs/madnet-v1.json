{
    "model" : {
        "main_name":            "TSD_MadNetv1",
        "base":                 "MadNet1",
        "base_params": {
            "alpha": 0.35
        },
        "downsample":           [32, 16, 8],
        "anchors":              [4,4, 5,6, 6,10, 6,4, 7,5, 7,7, 10,10, 16,16, 28,28],
        "labels":               [],
        "infer_shape":          [448, 448],
        "tiles":                1,
        "anchors_per_output":   3
    },

    "aug": {
        "enabled":              true,
        "shift_jitter":         0.3
    },

    "train": {
        "image_folder":         ["../data/RTSD_VOC_Train/Images/", "../data/GTSDB_VOC_Full/Images/"],
        "annot_folder":         ["../data/RTSD_VOC_Train/Annotations/", "../data/GTSDB_VOC_Full/Annotations/"],
        "cache_name":           "",

        "min_input_size":       [320, 320],
        "max_input_size":       [480, 480],
        
        "train_times":          1,
        "batch_size":           16,
        "nb_epochs":            500,
        "warmup_epochs":        0,
        "obj_thresh":           0.5,
        "iou_thresh":           0.5,
        "nms_thresh":           0.5,
        "gpus":                 "0",
        
        "optimizer":            "Nadam",
        "learning_rate":        1e-3,
        "lr_decay":             0,
        "clipnorm":             0,
        
        "grid_scales":          [1,1,1],
        "obj_scale":            5,
        "noobj_scale":          1,
        "xywh_scale":           1,
        "class_scale":          1,

        "debug":                false,
        "freeze":               false,
        "load_src_weights":     false
    },

    "valid": {
        "image_folder":         [],
        "annot_folder":         [],
        "cache_name":           "",

        "valid_times":          1
    },

    "eval": {
        "image_folder":         ["../data/RTSD_VOC_Test/Images/"],
        "annot_folder":         ["../data/RTSD_VOC_Test/Annotations/"],
        "cache_name":           ""
    }
}
